    "jsonwebtoken": "^8.2.0",
    "mathjs": "^5.4.2",
    "moment": "^2.23.0",
    "mongoose": "^5.0.12",
    "npm": "^6.4.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "prettier": "^1.14.3",
